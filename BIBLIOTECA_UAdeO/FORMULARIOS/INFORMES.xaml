<Window x:Class="BIBLIOTECA_UAdeO.FORMULARIOS.INFORMES"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BIBLIOTECA_UAdeO.FORMULARIOS"
        mc:Ignorable="d"
        Title="INFORMES"        
        Height="720"
        Width="1280"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanMinimize">
    <Grid>
        <Grid.Background>
            <ImageBrush x:Name="BIBLIOTECARIO_GRID" ImageSource="/FORMULARIOS/IMAGENES/BIBLIOTECARIO (2).png"/>
        </Grid.Background>
        <Button x:Name="BTNASISTENCIAINFORME" Margin="488,248,488,275" Click="BTNASISTENCIAINFORME_Click">
            <StackPanel Orientation="Horizontal" Height="83" Width="154">
                <Image Source="/FORMULARIOS/IMAGENES/ASISTENCIA.png" Width="48" Height="57"/>
                <TextBlock Width="76" TextAlignment="Center" Height="34"><Run Text="ASISTENCIAS"/></TextBlock>
            </StackPanel>
        </Button>
        <Button x:Name="BTNLIBROSINFORME" Margin="88,248,888,275" Click="BTNLIBROSINFORME_Click">
            <StackPanel Orientation="Horizontal">
                <Image Source="IMAGENES/LIBRO.png" Width="94" Height="71"/>
                <TextBlock Width="71"><Run/><LineBreak/><Run/><LineBreak/><Run Text="   "/><Run Text="LIBROS"/><LineBreak/><Run/></TextBlock>
            </StackPanel>
        </Button>
        <Button x:Name="BTNPRESTAMOS" Margin="858,248,118,275" Click="BTNPRESTAMOS_Click">
            <StackPanel Orientation="Horizontal">
                <Image Source="/FORMULARIOS/IMAGENES/PRESTAMOS.png" Width="74" Height="71"/>
                <TextBlock Width="103"><Run/><LineBreak/><Run/><LineBreak/><Run Text="   "/><Run Text="PR"/><Run Language="es-mx" Text="ÉS"/><Run Text="TAMOS"/><LineBreak/><Run/></TextBlock>
            </StackPanel>
        </Button>
        <Button x:Name="BTNSALIR" Margin="938,544,118,73" Click="BTNSALIR_Click">
            <StackPanel Orientation="Horizontal">
                <Image Source="/FORMULARIOS/IMAGENES/SALIR.png" Width="30" Height="30" Margin="0,0,5,0"/>
                <TextBlock Text="SALIR"/>
            </StackPanel>
        </Button>
    </Grid>
</Window>
